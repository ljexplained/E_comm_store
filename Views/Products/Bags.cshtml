    <!--================ Inspired Product Area =================-->
    <section id="shopnow" class="inspired_product_area section_gap_bottom_custom mt-40">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-12">
                    <div class="main_title">
                        <h2><span>Results</span></h2>

                    </div>
                </div>
            </div>

            <div class="row">
                @if (box != null)
                {
                    foreach (var item in box)
                    {



                        <div class="col-lg-3 col-md-4 col-sm-6 col-6">
                            <div class="single-product">
                                <div class="product-img">
                                    <a href="~/Products/Details?id=@item.Id&name=@item.Premark"><img class="img-fluid w-100" src="~/images/product/@item.img" alt="" /></a>

                                </div>
                                <div class="product-btm">
                                    <a href="~/Products/Details?id=@item.Id&name=@item.Premark" class="d-block">
                                        <h4>@item.Pname</h4>
                                    </a>
                                    <div class="mt-3">
                                        <span class="mr-4"><span>Rs&nbsp;</span>@item.salePrice<span>.00</span></span>

                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                }

                else
                {
                    <h1>Product Not Found</h1>
                }
            </div>
        </div>
    </section>
